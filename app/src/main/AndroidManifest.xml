<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rajpratim.completehealthcare">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Calories_detect.ActivityTracker"
            android:label="@string/title_activity_activity_tracker"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Food_t.Food_tracker"
            android:label="@string/title_activity_food_tracker"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Calories_detect.ECG.HealthCheckActivity"
            android:label="@string/title_activity_health_check"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Calories_detect.ECG.Calculate"
            android:label="@string/title_activity_health_check"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Calories_detect.ECG.BluetoothActivity"
            android:label="@string/title_activity_health_check"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
     ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".Calories_detect.StatActivity"></activity>
    </application>

</manifest>